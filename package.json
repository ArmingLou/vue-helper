{
  "name": "vue-vue-helper",
  "version": "2.5.0",
  "description": "every thing for speed",
  "keywords": [
    "vue-vue-helper",
    "vue",
    "vscode"
  ],
  "icon": "asset/logo.png",
  "publisher": "shenjiaolong",
  "author": "shenjiaolong",
  "homepage": "https://vue-vue-helper.80fight.cn/",
  "license": "MIT",
  "main": "dist/client",
  "l10n": "./l10n",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "publishConfig": {
    "registry": "https://registry.npm.taobao.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiaolong1021/vue-vue-helper.git"
  },
  "gitHead": "b1cc40df44b9cd73ba96d8b7f0369cc3ce690c45",
  "scripts": {
    "dev": "concurrently \"npm:dev-tsc\" \"npm:dev-build\"",
    "dev-tsc": "tsc -b tsconfig.build.json -w",
    "dev-build": "node scripts/build -- --watch",
    "build": "node scripts/build",
    "prepack:node": "npm run prebuild && npm run build:node -- --minify",
    "release": "npm run prepack:node && npm run publish",
    "publish": "vsce package",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "*",
    "onLanguage:vue"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vue-helper.searchFile",
        "title": "%extentions.searchFile%"
      },
      {
        "command": "vue-helper.backspace",
        "title": "vue-helper: backspace"
      },
      {
        "command": "vue-helper.blockSelect",
        "title": "vue-helper: blockSelect"
      },
      {
        "command": "vue-helper.funcEnhance",
        "title": "vue-helper: funcEnhance"
      }
    ],
    "keybindings": [
      {
        "key": "alt+x",
        "command": "vue-helper.blockSelect",
        "when": "editorTextFocus"
      },
      {
        "key": "shift+alt+enter",
        "command": "vue-helper.funcEnhance",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "@types/pg": "^8.11.4",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/l10n-dev": "^0.0.35",
    "axios": "^0.21.1",
    "camelcase": "^7.0.1",
    "crypto-js": "^4.1.1",
    "esbuild": "0.15.18",
    "esbuild-plugin-copy": "^2.0.2",
    "execa": "^6.1.0",
    "find-chrome-bin": "^1.0.2",
    "jsonc-parser": "^3.2.0",
    "mitt": "^3.0.0",
    "opn": "^6.0.0",
    "pretty": "^2.0.0",
    "puppeteer-core": "^19.7.0",
    "uuid": "^9.0.0",
    "vsce": "latest"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.18",
    "concurrently": "^8.2.2",
    "glob": "^10.3.12",
    "minio": "^7.1.3",
    "mysql2": "^3.9.3",
    "pg": "^8.11.3"
  }
}
